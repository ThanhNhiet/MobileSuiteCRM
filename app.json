{
  "expo": {
    "name": "crmmobile",
    "slug": "crmmobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/suitecrm_avatar.png",
    "scheme": "crmmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.android.suitecrm",
      "googleServicesFile": "./protection/google-services.json",
      "permissions": [
        "NOTIFICATIONS",
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/suitecrm_avatar.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/assets/images/suitecrm_avatar.png"
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-notifications",
        {
          "icon": "./src/assets/images/suitecrm_avatar.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      "@react-native-firebase/app",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/suitecrm_avatar.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "bb6dc6c9-614c-48cd-8b78-0bb4b2f20950"
      }
    },
    "owner": "thanhnhiet"
  }
}
